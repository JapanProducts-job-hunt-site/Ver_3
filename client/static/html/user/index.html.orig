<!--
    Landing Page for Students
    Requirements:
    * Explanation of this website (Example: Show the list of companies registered)
    * Sign In
    * Sign Up
    * Link to Landing Page for Companies
-->

<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1, user-scalable=0">

    <!-- <link rel="icon" href="img/favicon.png"> -->
    <title>For Students | Job Hunt Japan</title>

    <!-- CSS -->
    <link rel="stylesheet" type="text/css" href="static/css/styles.css">
    <link rel="stylesheet" type="text/css" href="static/css/landing.css">
</head>

<body>
    <div class="navbar">
        <a class="logo" href="">
            <img src="static/img/job-hunt-logo.png" alt="Job Hunt Japan Logo">
        </a>
        <p class="links">
            <a href="/">For Students</a>
            <a href="/company">For Companies</a>
        </p>
    </div>
    <div class="navbar-break"></div>
    <div class="bg"></div>

    <div class="signup-container">
        <form id="signup" action="">
            <!-- Do not delete ids or classnames; signup.js will not work -->
            <table>
                <tr>
                    <td colspan="2">
                        <h4>New Student?</h4>
                        <h1>Sign Up</h1>
                    </td>
                </tr>
                <tr>
                    <td>
                        <p>First Name</p>
                        <input class="firstname" type="text" name="firstname" required>
                    </td>
                    <td>
                        <p>Last Name</p>
                        <input class="lastname" type="text" name="lastname" required>
                    </td>
                    </td>
                </tr>
                <tr>
                    <td colspan="2">
                        <p>Email</p>
                        <input class="email" type="email" name="email" required>
                    </td>
                </tr>
                <tr>
                    <td colspan="2">
                        <p>Password</p>
                        <input class="password" type="password" name="password" required>
                    </td>
                </tr>
                <tr>
                    <td colspan="2">
                        <p>Confirm Password</p>
                        <input class="password_confirm" type="password" name="password_confirm" required>
                        <p class="password_validation">Passwords do not match.</p>
                    </td>
                </tr>
                <tr>
                    <td colspan="2" style="text-align: center;">
                        <input type="submit" name="signup" value="Sign Up">
                    </td>
                </tr>
            </table>
    </div>

